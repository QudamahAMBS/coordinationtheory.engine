#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
swrla:isRuleEnabled rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#hasConcurrentProcess
ct:hasConcurrentProcess rdf:type owl:ObjectProperty ,
                                 owl:SymmetricProperty ,
                                 owl:IrreflexiveProperty .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#hasDescription
ct:hasDescription rdf:type owl:ObjectProperty ,
                           owl:FunctionalProperty .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#hasFinishProcess
ct:hasFinishProcess rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf ct:hasSubProcess ;
                    owl:inverseOf ct:isFinishProcessOf ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:domain ct:Process ;
                    rdfs:range ct:Process .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#hasFlowDependency
ct:hasFlowDependency rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf ct:hasSubsequentProcess ;
                     owl:inverseOf ct:isFlowDependencyOf ;
                     rdfs:domain ct:Process ;
                     rdfs:range ct:Process .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#hasInputResource
ct:hasInputResource rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf ct:hasResource ;
                    owl:inverseOf ct:isInputResourceOf ;
                    rdfs:domain ct:Process ;
                    rdfs:range ct:Resource .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#hasLongestProcess
ct:hasLongestProcess rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf ct:hasSubProcess ;
                     owl:inverseOf ct:isLongestProcessOf ;
                     rdfs:domain ct:Process ;
                     rdfs:range ct:Process .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#hasOutputResource
ct:hasOutputResource rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf ct:hasResource ;
                     owl:inverseOf ct:isOutputResouceOf ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain ct:Process ;
                     rdfs:range ct:Resource .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#hasResource
ct:hasResource rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#hasStartProcess
ct:hasStartProcess rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf ct:hasSubProcess ;
                   owl:inverseOf ct:isStartProcessOf ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain ct:Process ;
                   rdfs:range ct:Process .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#hasSubProcess
ct:hasSubProcess rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf ct:hasSubProcessTransitive ;
                 owl:inverseOf ct:isSubProcessOf ;
                 rdfs:domain ct:Process ;
                 rdfs:range ct:Process ;
                 rdfs:label "has Sub-Process" .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#hasSubProcessTransitive
ct:hasSubProcessTransitive rdf:type owl:ObjectProperty ;
                           owl:inverseOf ct:isSubProcessOfTransitive ;
                           rdf:type owl:TransitiveProperty ;
                           rdfs:comment "Using the same naming convention as SKOS to distinguish between direct sub-processes and transitive sub-processes" .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#hasSubsequentProcess
ct:hasSubsequentProcess rdf:type owl:ObjectProperty ;
                        owl:inverseOf ct:isSubsequentProcessOf ;
                        rdfs:domain ct:Process ;
                        rdfs:range ct:Process ;
                        rdfs:comment "Subsequent processes are ALL downstream processes, i.e., transitive closure of hasFlowDependency)" .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#ifFalseHasOutput
ct:ifFalseHasOutput rdf:type owl:ObjectProperty ;
                    rdfs:domain ct:DecisionProcess ;
                    rdfs:range ct:Process .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#ifTrueHasOutput
ct:ifTrueHasOutput rdf:type owl:ObjectProperty ;
                   rdfs:domain ct:DecisionProcess ;
                   rdfs:range ct:Process .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#isFinishProcessOf
ct:isFinishProcessOf rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf ct:isSubProcessOf ;
                     rdf:type owl:FunctionalProperty .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#isFlowDependencyOf
ct:isFlowDependencyOf rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf ct:isSubsequentProcessOf .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#isInputResourceOf
ct:isInputResourceOf rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf ct:isResourceOf .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#isLongestProcessOf
ct:isLongestProcessOf rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf ct:isSubProcessOf ;
                      rdf:type owl:FunctionalProperty .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#isOutputResouceOf
ct:isOutputResouceOf rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf ct:isResourceOf .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#isResourceOf
ct:isResourceOf rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#isStartProcessOf
ct:isStartProcessOf rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf ct:isSubProcessOf ;
                    rdf:type owl:FunctionalProperty .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#isSubProcessOf
ct:isSubProcessOf rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf ct:isSubProcessOfTransitive .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#isSubProcessOfTransitive
ct:isSubProcessOfTransitive rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#isSubsequentProcessOf
ct:isSubsequentProcessOf rdf:type owl:ObjectProperty ,
                                  owl:TransitiveProperty ;
                         rdfs:comment "Subsequent processes are ALL upstream  processes, i.e., transitive closure of isFlowDependencyOf)" .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#isTaskOf
ct:isTaskOf rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#durationBeforeDeadline
ct:durationBeforeDeadline rdf:type owl:DatatypeProperty ;
                          rdfs:range xsd:decimal .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#durationFromStart
ct:durationFromStart rdf:type owl:DatatypeProperty ;
                     rdfs:domain ct:Process ;
                     rdfs:range xsd:decimal .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#durationInWorkDays
ct:durationInWorkDays rdf:type owl:DatatypeProperty ;
                      rdfs:domain ct:Process ;
                      rdfs:range xsd:decimal .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#processDecisionValue
ct:processDecisionValue rdf:type owl:DatatypeProperty ;
                        rdfs:domain ct:DecisionProcess ;
                        rdfs:range xsd:boolean ;
                        rdfs:comment "A boolean for a process decision that controls which process to go to next. This is a simple yes/no example for now." .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#slack
ct:slack rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain ct:Process ;
         rdfs:range xsd:decimal .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#Actor
ct:Actor rdf:type owl:Class ;
         rdfs:subClassOf ct:Resource .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#AgileDomain
ct:AgileDomain rdf:type owl:Class .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#BehindScheduleProcess
ct:BehindScheduleProcess rdf:type owl:Class ;
                         owl:equivalentClass [ owl:intersectionOf ( ct:Process
                                                                    [ rdf:type owl:Restriction ;
                                                                      owl:onProperty ct:slack ;
                                                                      owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                           owl:onDatatype xsd:decimal ;
                                                                                           owl:withRestrictions ( [ xsd:maxExclusive 0
                                                                                                                  ]
                                                                                                                )
                                                                                         ]
                                                                    ]
                                                                  ) ;
                                               rdf:type owl:Class
                                             ] .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#CallForTender
ct:CallForTender rdf:type owl:Class ;
                 rdfs:subClassOf ct:ManufacturingCollaborationDomain ,
                                 ct:Resource ;
                 rdfs:comment "A request for a Tender. A Tender is a bid/ proposal in the tendering process." .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#DecisionProcess
ct:DecisionProcess rdf:type owl:Class ;
                   rdfs:subClassOf ct:Process .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#Iteration
ct:Iteration rdf:type owl:Class ;
             rdfs:subClassOf ct:AgileDomain ,
                             ct:Process .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#ManufacturingCollaborationDomain
ct:ManufacturingCollaborationDomain rdf:type owl:Class .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#ParallelProcess
ct:ParallelProcess rdf:type owl:Class ;
                   rdfs:subClassOf ct:Process .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#Part
ct:Part rdf:type owl:Class ;
        rdfs:subClassOf ct:Product .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#Process
ct:Process rdf:type owl:Class ;
           rdfs:subClassOf ct:ProcessDomain .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#ProcessDomain
ct:ProcessDomain rdf:type owl:Class .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#Product
ct:Product rdf:type owl:Class ;
           rdfs:subClassOf ct:Resource .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#Resource
ct:Resource rdf:type owl:Class ;
            rdfs:subClassOf ct:ProcessDomain .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#SequentialProcess
ct:SequentialProcess rdf:type owl:Class ;
                     rdfs:subClassOf ct:Process .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#Service
ct:Service rdf:type owl:Class ;
           rdfs:subClassOf ct:Resource .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#Task
ct:Task rdf:type owl:Class ;
        rdfs:subClassOf ct:AgileDomain ,
                        ct:Process .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#Tender
ct:Tender rdf:type owl:Class ;
          rdfs:subClassOf ct:ManufacturingCollaborationDomain ,
                          ct:Resource ;
          rdfs:comment "A bid or proposal. Usually in response to a Call For Tender (CFT)." .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#AssembleProductActivities
ct:AssembleProductActivities rdf:type owl:NamedIndividual ,
                                      ct:ManufacturingCollaborationDomain ;
                             ct:hasSubProcess ct:Incorporate-G1-G2 ,
                                              ct:PartnerProvideInput-G2 ;
                             rdfs:label "Assemble Product Activities" .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#CFT1
ct:CFT1 rdf:type owl:NamedIndividual ,
                 ct:CallForTender ;
        ct:hasFlowDependency ct:t1CreateTenderDraftTask ;
        ct:isTaskOf ct:_LeadSupplier ;
        rdfs:label "CFT1" .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#CodingAndUnitTesting
ct:CodingAndUnitTesting rdf:type owl:NamedIndividual ,
                                 ct:ProcessDomain ,
                                 ct:Task ;
                        ct:hasFlowDependency ct:IntegrationAndTesting ;
                        ct:hasSubsequentProcess ct:IntegrationAndTesting ;
                        ct:isFlowDependencyOf ct:SystemDesign ;
                        ct:isSubProcessOf ct:ExampleWaterfallModel ;
                        ct:isSubsequentProcessOf ct:RequirementsAnalysis ,
                                                 ct:SystemDesign ;
                        ct:durationFromStart 340.0 ;
                        ct:durationInWorkDays 180.0 .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#CoordinateActivityVotingToResolveSharedInputDependencies
ct:CoordinateActivityVotingToResolveSharedInputDependencies rdf:type owl:NamedIndividual ,
                                                                     ct:ManufacturingCollaborationDomain ;
                                                            ct:hasSubProcess ct:EnsureAgreement-G1-G2 ,
                                                                             ct:PartnerVoteFor-G1-G2 ;
                                                            rdfs:label "Coordinate Activity Voting To Resolve Shared Input Dependencies" .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#DefineRequirementForModule1
ct:DefineRequirementForModule1 rdf:type owl:NamedIndividual ,
                                        ct:Process ,
                                        ct:Task ;
                               ct:isSubProcessOf ct:DefineRequirements ,
                                                 ct:ExampleWaterfallModel ,
                                                 ct:RequirementsAnalysis ;
                               ct:durationFromStart 40.0 .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#DefineRequirementForModule2
ct:DefineRequirementForModule2 rdf:type owl:NamedIndividual ,
                                        ct:Process ,
                                        ct:Task ;
                               ct:isSubProcessOf ct:DefineRequirements ,
                                                 ct:ExampleWaterfallModel ,
                                                 ct:RequirementsAnalysis ;
                               ct:durationFromStart 30.0 .


###  http://www.semanticweb.org/mdebellis/ontologies/2017/4/cordination-theory#DefineRequirementForModule3
ct:DefineRequirementForModule3 rdf:type owl:NamedIndividual ,
                                        ct:Process ,
                                        ct:Task ;
                               ct:isFinishProcessOf ct:DefineRequirements ;
                               ct:isLongestProcessOf ct:DefineRequirements ;
                               ct:isSubProcessOf ct:DefineRequirements ,
                                                 ct:ExampleWaterfallModel ,
