#################################################################
#    Rules
#################################################################

ct:pd rdf:type swrl:Variable .

ct:p1 rdf:type swrl:Variable .

ct:p11 rdf:type swrl:Variable .

ct:p2 rdf:type swrl:Variable .

ct:p21 rdf:type swrl:Variable .

ct:r rdf:type swrl:Variable .

ct:fnp rdf:type swrl:Variable .

[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "If the deicison proces boolean is true then consensus was achieved and the output is Tender Agreed" ;
   rdfs:label "DecisionProcessTrue" ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:ClassAtom ;
                           swrl:classPredicate ct:DecisionProcess ;
                           swrl:argument1 ct:pd
                         ] ;
               rdf:rest [ rdf:type swrl:AtomList ;
                          rdf:first [ rdf:type swrl:DatavaluedPropertyAtom ;
                                      swrl:propertyPredicate ct:processDecisionValue ;
                                      swrl:argument1 ct:pd ;
                                      swrl:argument2 "true"^^xsd:boolean
                                    ] ;
                          rdf:rest rdf:nil
                        ]
             ] ;
   swrl:head [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate ct:hasOutputResource ;
                           swrl:argument1 ct:pd ;
                           swrl:argument2 ct:Tender1Final
                         ] ;
               rdf:rest rdf:nil
             ]
 ] .

[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "If there is a flow dependency between two processes, then there is a flow dependency for their super process." ;
   rdfs:label "FlowDependenciesSubProcesses" ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate ct:hasSubProcess ;
                           swrl:argument1 ct:p1 ;
                           swrl:argument2 ct:p11
                         ] ;
               rdf:rest [ rdf:type swrl:AtomList ;
                          rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                      swrl:propertyPredicate ct:hasSubProcess ;
                                      swrl:argument1 ct:p2 ;
                                      swrl:argument2 ct:p21
                                    ] ;
                          rdf:rest [ rdf:type swrl:AtomList ;
                                     rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                 swrl:propertyPredicate ct:hasFlowDependency ;
                                                 swrl:argument1 ct:p11 ;
                                                 swrl:argument2 ct:p21
                                               ] ;
                                     rdf:rest [ rdf:type swrl:AtomList ;
                                                rdf:first [ rdf:type swrl:DifferentIndividualsAtom ;
                                                            swrl:argument1 ct:p1 ;
                                                            swrl:argument2 ct:p2
                                                          ] ;
                                                rdf:rest rdf:nil
                                              ]
                                   ]
                        ]
             ] ;
   swrl:head [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate ct:hasFlowDependency ;
                           swrl:argument1 ct:p1 ;
                           swrl:argument2 ct:p2
                         ] ;
               rdf:rest rdf:nil
             ]
 ] .

[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "If the output resource of process1 is the input resource of process2 there is a flow dependency between 1 and 2" ;
   rdfs:label "FlowDependency" ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate ct:hasOutputResource ;
                           swrl:argument1 ct:p1 ;
                           swrl:argument2 ct:r
